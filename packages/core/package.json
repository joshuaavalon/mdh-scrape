{
  "name": "@mdhs/core",
  "version": "1.0.0",
  "description": "Core package for Media Data Hub scraper",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--enable-source-maps jest"
  },
  "imports": {
    "#episode": {
      "types": "./dist/episode/index.d.ts",
      "import": "./dist/episode/index.js"
    },
    "#error": {
      "types": "./dist/error/index.d.ts",
      "import": "./dist/error/index.js"
    },
    "#type": {
      "types": "./dist/type/index.d.ts",
      "import": "./dist/type/index.js"
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "homepage": "https://github.com/mdhs/mdhs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdhs/mdhs.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/mdhs/mdhs/issues"
  },
  "keywords": [],
  "files": [
    "dist",
    "!dist/**/__tests__/**/*"
  ],
  "author": "Joshua Avalon",
  "license": "Apache-2.0",
  "dependencies": {
    "@media-data-hub/sdk": "^1.1.2",
    "@types/luxon": "^3.4.2",
    "luxon": "^3.4.4",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "playwright": "^1.46.0"
  }
}
